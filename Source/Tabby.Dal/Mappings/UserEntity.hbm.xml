<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Tabby.Dal"
                   namespace="Tabby.Dal.Domain">

  <class name="UserEntity" table="[dbo].[User]">
    <id name="Id">
      <generator class="guid" />
    </id>
    <property name="Name" not-null="true" />
    <property name="IsOnline" not-null="true" />
    <bag name="DeliveredMessages" table="DeliveredMessage" inverse="true" lazy="true">
        <key column="Id"/>
        <many-to-many class="MessageEntity" column="MessageId"/>
    </bag>
  </class>
  
</hibernate-mapping>
